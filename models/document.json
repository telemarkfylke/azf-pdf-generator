{
  "$id": "document.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "default": {},
  "description": "Document required to generate document",
  "examples": [{
    "system": "minelev",
    "template": "varsel-fag",
    "language": "nb",
    "data": {}
  }],
  "required": [
    "system",
    "template"
  ],
  "title": "The root schema",
  "type": "object",
  "properties": {
    "system": {
      "$id": "#/properties/system",
      "type": "string",
      "title": "System",
      "description": "Systemet som \"eier\" malen",
      "examples": [
        "minelev"
      ]
    },
    "template": {
      "$id": "#/properties/template",
      "type": "string",
      "title": "Template",
      "description": "Malnavn",
      "examples": [
        "varsel-fag"
      ]
    },
    "language": {
      "$id": "#/properties/language",
      "default": "nb",
      "description": "Spr√•k",
      "examples": [
        "nb"
      ],
      "title": "Language",
      "enum": [
        "nb",
        "nn",
        "en"
      ],
      "type": "string"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "Data object",
      "description": "The data sent to the template. Varies based on system and template (and language).",
      "default": {},
      "examples": [{}],
      "required": [],
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}